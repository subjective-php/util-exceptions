{
    "name": "subjective-php/util-exceptions",
    "description": "Collection of exception utilities",
    "keywords": ["exception", "util"],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/yaml": ">=2.0.5"
    },
    "autoload": {
        "psr-4": { "Chadicus\\Util\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "ChadicusTest\\Util\\": "tests/" }
    }
}
